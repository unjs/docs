{
  "name": "unjs-docs",
  "version": "0.1.8",
  "license": "MIT",
  "type": "module",
  "exports": {},
  "bin": {
    "unjs-docs": "./dist/cli.mjs"
  },
  "files": [
    "dist",
    "app",
    "!app/.nuxt",
    "!app/node_modules",
    "!app/tsconfig.json",
    "schema"
  ],
  "scripts": {
    "build": "bun build-schema && unbuild",
    "build-schema": "typescript-json-schema ./schema/config.d.ts DocsConfig -o schema/config.json",
    "dev": "NUXT_DOCS_DEV=1 bun unjs-docs dev playground",
    "dev:build": "bun unjs-docs build playground",
    "dev:ssr": "NUXT_DOCS_SSR=1 bun dev",
    "dev:template": "bun unjs-docs dev template",
    "types:prepare": "NUXT_DOCS_PREPARE=1 nuxt prepare app",
    "lint": "eslint --cache --ext .ts,.js,.mjs,.cjs . && prettier -c src",
    "lint:fix": "eslint --cache --ext .ts,.js,.mjs,.cjs . --fix && prettier -c src -w",
    "prepack": "bun run build",
    "release": "bun test && changelogen --release && npm publish && git push --follow-tags",
    "test": "bun lint",
    "unjs-docs": "JITI_ESM_RESOLVE=1 jiti ./src/cli.ts"
  },
  "dependencies": {
    "@nuxt/content": "^2.11.0",
    "@nuxt/ui": "^2.13.0",
    "@nuxt/ui-pro": "npm:@nuxt/ui-pro-edge@0.7.4-28445636.12065c9",
    "@nuxthq/studio": "^1.0.10",
    "@nuxtjs/fontaine": "^0.4.1",
    "@nuxtjs/google-fonts": "^3.1.3",
    "@nuxtjs/plausible": "^0.2.4",
    "@nuxtjs/seo": "2.0.0-rc.5",
    "@nuxtjs/tailwindcss": "^6.11.2",
    "c12": "^1.6.1",
    "citty": "^0.1.5",
    "consola": "^3.2.3",
    "defu": "^6.1.4",
    "is-buffer": "^2.0.5",
    "nuxi": "^3.10.0",
    "nuxt": "^3.10.0",
    "pkg-types": "^1.0.3",
    "tailwindcss": "^3.4.1",
    "theme-colors": "^0.1.0",
    "unstorage": "^1.10.1",
    "vue": "~3.3.13",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@nuxt/eslint-config": "^0.2.0",
    "@types/node": "^20.11.10",
    "changelogen": "^0.5.5",
    "eslint": "^8.56.0",
    "eslint-config-unjs": "^0.2.1",
    "jiti": "^1.21.0",
    "prettier": "^3.2.4",
    "scule": "^1.2.0",
    "typescript": "^5.3.3",
    "typescript-json-schema": "^0.62.0",
    "unbuild": "^2.0.0",
    "vue-tsc": "^1.8.27"
  }
}
